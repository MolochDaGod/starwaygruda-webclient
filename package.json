{
  "name": "starwaygruda-webclient",
  "version": "1.0.0",
  "description": "Web-based 3D client for StarWayGRUDA MMO - renders original SWG terrain and assets",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vercel",
    "agent": "node game-vision-agent.js",
    "agent:watch": "node game-vision-agent.js watch",
    "bridge": "node server/swgemu-bridge.js",
    "warp": "node warp-ambient-worker.js",
    "start:all": "concurrently \"npm run bridge\" \"npm run dev\"",
    "start:full": "concurrently \"npm run warp\" \"npm run bridge\" \"npm run dev\""
  },
  "keywords": [
    "swgemu",
    "mmo",
    "threejs",
    "webgl",
    "metaverse"
  ],
  "author": "StarWayGRUDA Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@jscad/modeling": "^2.12.6",
    "@react-three/postprocessing": "^3.0.4",
    "@tweenjs/tween.js": "^25.0.0",
    "axios": "^1.6.5",
    "binary-parser": "^2.3.0",
    "buffer": "^6.0.3",
    "cannon-es": "^0.20.0",
    "choco": "^0.2.1",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "earcut": "^3.0.2",
    "express": "^4.22.1",
    "jszip": "^3.10.1",
    "lil-gui": "^0.21.0",
    "monaco-editor": "^0.55.1",
    "pako": "^2.1.0",
    "poly2tri": "^1.5.0",
    "simplex-noise": "^4.0.3",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.6.1",
    "stats.js": "^0.17.0",
    "three": "^0.160.0",
    "three-csg-ts": "^3.2.0",
    "three-mesh-bvh": "^0.9.5",
    "three-stdlib": "^2.36.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^5.4.11"
  }
}
